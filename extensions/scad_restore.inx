<?xml version="1.0" encoding="UTF-8"?>
<inkscape-extension xmlns="http://www.inkscape.org/namespace/inkscape/extension">
    <_name>Restore</_name>
    <id>org.ekips.filter.scad_restore</id>
    <dependency type="executable" location="extensions">inkex.py</dependency>
    <param name="restore" type="string" _gui-text="Scad parameters filename">parameters.scd.json</param>
    <effect needs-live-preview="false">
        <object-type>all</object-type>
        <effects-menu>
            <submenu _name="Scad parameters"/>
        </effects-menu>
    </effect>
    <script>
        <command reldir="extensions" interpreter="python">scad_restore.py</command>
    </script>
</inkscape-extension>
